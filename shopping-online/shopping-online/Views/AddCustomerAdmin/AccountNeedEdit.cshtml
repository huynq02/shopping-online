                    <div class="col-md-8">
                        <section class="panel tasks-widget">
                            <header class="panel-heading">
                                Edit Customer
                            </header>
                            @using (Html.BeginForm("Edit", "AddCustomerAdmin", FormMethod.Post))
                            {
                                <div class="panel-body">
                                    <form role="form">
                                        <div class="form-group">
                                            <label for="exampleInputEmail1">Full Name</label>
                                            @Html.EditorFor(model => model.acc.account_name, new { htmlAttributes = new { @class = "form-control" } })
                                        </div>
                                        <div class="form-group">
                                            <label for="exampleInputPassword1">Email</label>
                                            @Html.EditorFor(model => model.acc.account_email, new { htmlAttributes = new { @class = "form-control" } })
                                        </div>
                                        <div class="form-group">
                                            <label for="exampleInputPassword1">UserName</label>
                                            @Html.EditorFor(model => model.acc.account_username, new { htmlAttributes = new { @class = "form-control" } })
                                        </div>
                                        <div class="form-group">
                                            <label for="exampleInputPassword1">PassWord</label>
                                            @Html.EditorFor(model => model.acc.account_password, new { htmlAttributes = new { @class = "form-control" } })
                                        </div>
                                        <div class="form-group">
                                            <label for="exampleInputPassword1">Phone</label>
                                            @Html.EditorFor(model => model.acc.account_phone, new { htmlAttributes = new { @class = "form-control" } })
                                        </div>
                                        <div class="form-group">
                                            <label for="exampleInputPassword1">Gender</label> <br />
                                            @if (Model.acc.account_gender == true)
                                            {
                                                <input type="radio" name="gender" value="Male" checked /><b>Male</b>
                                                <input type="radio" name="gender" value="Female" /><b>Female</b>
                                            }
                                            @if (Model.acc.account_gender == false)
                                            {
                                                <input type="radio" name="gender" value="Male" /><b>Male</b>
                                                <input type="radio" name="gender" value="Female" checked /><b>Female</b>
                                            }
                                        </div>
                                        <div class="form-group">
                                            <label for="exampleInputPassword1">Address</label> <br />
                                            @Html.EditorFor(model => model.acc.account_address, new { htmlAttributes = new { @class = "form-control" } })
                                        </div>
                                        <div class="form-group">
                                            <label for="exampleInputPassword1">Status_Active</label> <br />
                                            @if (Model.acc.account_status == true)
                                            {
                                                <input type="radio" name="status" value="Active" checked /><b>Ative</b>
                                                <input type="radio" name="status" value="Deactive" /><b>De-Active</b>
                                            }
                                            @if (Model.acc.account_status == false)
                                            {
                                                <input type="radio" name="status" value="Active" /><b>Active</b>
                                                <input type="radio" name="status" value="Deactive" checked /><b>De-Active</b>
                                            }
                                        </div>
                                        <div class="form-group">
                                            <label for="exampleInputPassword1">Create Date</label> <br />
                                            @Html.EditorFor(model => model.acc.account_createdate, new { htmlAttributes = new { @class = "form-control" } })
                                        </div>
                                        <input type="hidden" name="Id" value="@Model.acc.account_id" />
                                        <button type="submit" class="btn btn-info">Save</button>
                                    </form>

                                </div>
                            }
                        </section>
                    </div>
                </div>
                <div class="row">

                    <div class="col-md-8">
                        <section class="panel">
                            <header class="panel-heading">
                                Order History
                            </header>
                            <div class="panel-body table-responsive">
                                <table class="table table-hover">
                                    <thead>
                                        <tr>
                                            <th>Product Name</th>
                                            <th>Quantity</th>
                                            <th>Price</th>
                                            <th>Order Date</th>
                                            <th>Status</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var i in Model.product)
                                        {
                                            foreach (var j in Model.lstOrderCus)
                                            {
                                                if (i.product_id == j.product_id)
                                                {
                                                    <tr>
                                                        <td>@i.product_name</td>
                                                        <td>@j.Order_Details_num</td>
                                                        <td>@j.Order_Details_price</td>
                                                        @foreach (var k in Model.lstOrder)
                                                        {
                                                            if (k.Order_id == j.Order_id)
                                                            {
                                                                <td>@k.Order_Date</td>
                                                                foreach (var l in Model.lstOrderStatus)
                                                                {
                                                                    if (l.Order_status_id == k.Order_status_id)
                                                                    {
                                                                        <td>@l.Order_status_status</td>
                                                                    }
                                                                }

                                                            }
                                                        }

                                                    </tr>
                                                }
                                            }
                                        }
                                    </tbody>
                                </table>
                                @*@Html.PagedListPager(Model.account, page => Url.Action("Index", new { page }))*@
                            </div>
